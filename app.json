{
  "expo": {
    "name": "Todo app",
    "slug": "my-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/VTask_.png",
      "resizeMode": "contain",
      "backgroundColor": "#1C98ED"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/VTask.png",
        "backgroundColor": "#1C98ED"
      },
      "package": "com.victuk.myapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "cdc6771a-337b-4380-900e-3d3d6a63849d"
      }
    },
    "owner": "victuk"
  }
}
